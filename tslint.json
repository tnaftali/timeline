{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "trailing-comma": [false],
        "object-literal-sort-keys": false,
        "arrow-parens": [true],
        "radix": false,
        "ban-types": false,
        "no-var-requires": false,
        "no-console": false,
        "max-line-length": false,
        "variable-name": {
            "options": [
                "allow-snake-case"
            ]
        }
    },
    "rulesDirectory": []
}
